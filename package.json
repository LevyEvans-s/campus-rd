{
  "name": "campus-rd",
  "version": "1.0.0",
  "description": "阿里终端练习生-校园综合社交平台后端",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^3",
    "egg-scripts": "^2",
    "md5": "^2.3.0",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "egg-view-ejs": "^3.0.0",
    "mysql2": "^2.3.3",
    "egg-mysql": "^3.1.1"
  },
  "devDependencies": {
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-campus-rd",
    "stop": "egg-scripts stop --title=egg-server-campus-rd",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "ci": {
    "version": "16, 18",
    "type": "github"
  },
  "repository": {
    "type": "git",
    "url": ""
